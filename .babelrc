{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/transform-runtime",
    [
      "import", 
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",   // default: lib
        "style": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties", 
      { 
        "loose": true 
      }
    ]
  ]
}
